  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "Cookie Doctor",
    "default_popup": "index.html"
  },
  "permissions": [
    "management",
    "tabs",
    "https://cookie-doctor.herokuapp.com",
    "storage"
  ]
}